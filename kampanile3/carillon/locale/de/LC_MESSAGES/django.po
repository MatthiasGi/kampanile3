#, python-format
msgid "Carillon: %(name)s"
msgstr "Carillon: %(name)s"

#: .\carillon\views\carillon.py:55
msgid "Create carillon"
msgstr "Carillon erstellen"

#: .\carillon\views\carillon.py:56
msgid "Add a carillon to play notes on."
msgstr "Ein Carillon hinzufügen, um Noten darauf zu spielen:"

#: .\carillon\views\carillon.py:72
msgid "Modify carillon"
msgstr "Carillon bearbeiten"

#: .\carillon\views\carillon.py:73
#, python-format
msgid "Modify the settings of the carillon: %(name)s"
msgstr "Einstellungen des Carillons ändern: %(name)s"

