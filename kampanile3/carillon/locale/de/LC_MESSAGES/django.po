msgstr "Carillon löschen:"

#: .\carillon\templates\carillon\carillon_detail.html:7
#, python-format
msgid ""
"The carillon is connected to port <code>%(port)s</code>. You can play notes "
"on it."
msgstr ""
"Das Carillon ist mit dem Port <code>%(port)s</code> verbunden. Sie können "
"Noten darauf spielen."

#: .\carillon\templates\carillon\carillon_detail.html:10
msgid "Test playing notes"
msgstr "Noten testen"

#: .\carillon\templates\carillon\carillon_detail.html:42
msgid "Note played successfully!"
msgstr "Note erfolgreich gespielt!"

#: .\carillon\templates\carillon\carillon_detail.html:50
msgid "Error playing note: ${ data.error }"
msgstr "Fehler beim Spielen der Note: ${ data.error }"

