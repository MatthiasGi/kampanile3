server {
  listen 80 default_server;
  listen [::]:80 default_server;

  server_name _;

  location = /favicon.ico {
    access_log off;
    log_not_found off;
  }

  location /static/ {
    alias /var/www/kampanile3/static/;
  }

  location / {
    include proxy_params;
    proxy_pass http://unix:/run/gunicorn.sock;
  }
}
